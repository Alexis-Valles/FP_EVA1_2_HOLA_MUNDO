    public static void main(String[] args) {
        // TODO code application logic here
        System.out.println("HOLA MUNDO");  //Cadena de texto
        System.out.println(100);   //Un numero entero
        System.out.println('c');   //Un caracter
        System.out.println(true);  //Valores lógicos
        System.out.println(false);
        System.out.println(1000.52);  //Numeros cin fracción
        
        //TIPOS DE DATOS 
        //ENTEROS
        //int
        //byte
        //short
        //PUNTO FLOTNTE
        //double
        //float
        //TEXTO
        //String
        //CARACTER
        //char
        //LOGICOS (true false)
        //boolean
        
        //creación y uso de variable
        
        int edad; //Declaro una variable de tipo entero, llamada 
                  //edad
        //double edad;  //NO PODEMOS TENER DOS VARIABLES CON EL MISMO NOMBRE
        String nombre;
        String apellido;
        double promedio;
        
        edad = 18;  //estoya signando el valor 18 dentro de la 
                    //variable edad
        
        System.out.println(edad); //println tambien imprime variable
    }
    
    
}
